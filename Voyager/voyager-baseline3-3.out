    if (currentLogs >= targetCount) {
        bot.chat(`Successfully mined ${currentLogs} spruce logs. Task completed.`);
    } else {
        bot.chat(`Failed to mine ${targetCount} spruce logs. Only have ${currentLogs}.`);
