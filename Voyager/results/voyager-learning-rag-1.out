    // Check fuel (coal)
    const coal = mcData.itemsByName["coal"];
    let coalCount = bot.inventory.count(coal.id);
    if (coalCount < neededIngots) {
      const neededCoal = neededIngots - coalCount;
      bot.chat(`Need ${neededCoal} coal. Mining coal ore...`);
      // Explore for coal ore (underground direction)
      const foundCoal = await exploreUntil(bot, new Vec3(0, -1, 0), 60, () => {
        const coalOre = bot.findBlock({
          matching: mcData.blocksByName["coal_ore"].id,
          maxDistance: 32
        });
        return coalOre;
      });
      if (!foundCoal) {
        bot.chat("Could not find coal ore within 60 seconds.");
        return;
      }
      await mineBlock(bot, "coal_ore", neededCoal);
      coalCount = bot.inventory.count(coal.id);
    }

    // Smelt raw copper
    bot.chat("Smelting raw copper...");
    await smeltItem(bot, "raw_copper", "coal", neededIngots);
    ingotCount = bot.inventory.count(copperIngot.id);
    bot.chat(`Now have ${ingotCount} copper ingot(s).`);
