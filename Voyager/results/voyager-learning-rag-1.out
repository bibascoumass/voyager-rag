async function mineCopperOre(bot) {
    // Check if we already have a stone pickaxe
    const stonePickaxe = mcData.itemsByName.stone_pickaxe;
    if (bot.inventory.count(stonePickaxe.id) < 1) {
        bot.chat("No stone pickaxe. Need to craft one first.");
        // We could craft, but we have one in inventory.
        return;
    }
    // Equip stone pickaxe
    const pickaxeItem = bot.inventory.findInventoryItem(stonePickaxe.id);
    if (pickaxeItem) {
        await bot.equip(pickaxeItem, "hand");
        bot.chat("Equipped stone pickaxe.");
    } else {
        bot.chat("Stone pickaxe not found in inventory.");
        return;
    }
    // Find copper ore nearby
    let copperOreBlock = bot.findBlock({
        matching: mcData.blocksByName.copper_ore.id,
        maxDistance: 32
    });
    if (!copperOreBlock) {
        bot.chat("Copper ore not found nearby. Exploring...");
        // Random direction: choose between surface and underground
        const directions = [new Vec3(1, 0, 1), new Vec3(0, -1, 0)];
        const randomDir = directions[Math.floor(Math.random() * directions.length)];
        copperOreBlock = await exploreUntil(bot, randomDir, 60, () => {
            const block = bot.findBlock({
                matching: mcData.blocksByName.copper_ore.id,
                maxDistance: 32
            });
            return block;
        });
        if (!copperOreBlock) {
            bot.chat("Could not find copper ore within 60 seconds.");
            return;
        }
    }
    bot.chat("Found copper ore. Mining...");
    await mineBlock(bot, "copper_ore", 1);
    // Verify
    const rawCopper = mcData.itemsByName.raw_copper;
    const rawCount = bot.inventory.count(rawCopper.id);
    bot.chat(`Now have ${rawCount} raw copper.`);
    if (rawCount > 0) {
        bot.chat("Successfully mined copper ore.");
    } else {
        bot.chat("Failed to mine copper ore.");
    }
}
