async function mineWoodLogs(bot) {
    // Count current logs in inventory
    const logNames = ["oak_log", "spruce_log", "birch_log", "jungle_log", "acacia_log", "dark_oak_log", "mangrove_log", "cherry_log"];
    let totalLogs = 0;
    for (const name of logNames) {
        const item = mcData.itemsByName[name];
        if (item) {
            const logs = bot.inventory.count(item.id);
            totalLogs += logs;
        }
    }
    bot.chat(`I have ${totalLogs} logs.`);
    if (totalLogs >= 3) {
        bot.chat("Already have at least 3 logs.");
        return;
    }
    const needed = 3 - totalLogs;
    bot.chat(`Need to mine ${needed} more logs.`);
    // Mine oak_log as it's common; if not found, explore
    await exploreUntil(bot, new Vec3(1, 0, 1), 60, () => {
        const logBlock = bot.findBlock({
            matching: (block) => logNames.includes(block.name),
            maxDistance: 32,
        });
        return logBlock;
    });
    await mineBlock(bot, "oak_log", needed);
    // Verify
    totalLogs = 0;
    for (const name of logNames) {
        const item = mcData.itemsByName[name];
        if (item) {
            const logs = bot.inventory.count(item.id);
            totalLogs += logs;
        }
    }
    bot.chat(`Now I have ${totalLogs} logs.`);
}
