async function mineOneWoodLog(bot) {
    // Check if we already have a wood log in inventory
    const logNames = ["oak_log", "birch_log", "spruce_log", "jungle_log", "acacia_log", "dark_oak_log", "mangrove_log"];
    let hasLog = false;
    for (const logName of logNames) {
        const logItem = mcData.itemsByName[logName];
        if (bot.inventory.findInventoryItem(logItem.id)) {
            hasLog = true;
            break;
        }
    }
    if (hasLog) {
        bot.chat("Already have a wood log.");
        return;
    }

    // Explore until we find a wood log
    const direction = new Vec3(Math.floor(Math.random() * 3) - 1, 0, Math.floor(Math.random() * 3) - 1); // Random direction
    const logBlock = await exploreUntil(bot, direction, 60, () => {
        for (const logName of logNames) {
            const block = bot.findBlock({
                matching: mcData.blocksByName[logName]?.id,
                maxDistance: 32,
            });
            if (block) return bot.blockAt(block);
        }
        return null;
    });
    if (!logBlock) {
        bot.chat("Could not find a wood log within exploration time.");
        return;
    }

    // Mine one log
    await mineBlock(bot, logBlock.name, 1);
    bot.chat("Mined 1 wood log.");
}
