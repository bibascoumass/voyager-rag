async function smeltCopperOre(bot) {
  // Check if already have copper ingot
  const copperIngot = mcData.itemsByName.copper_ingot;
  if (bot.inventory.count(copperIngot.id) >= 1) {
    bot.chat("Already have a copper ingot.");
    return;
  }

  // Check for raw_copper
  const rawCopper = mcData.itemsByName.raw_copper;
  let rawCount = bot.inventory.count(rawCopper.id);
  if (rawCount < 1) {
    bot.chat("No raw copper. Need to mine copper ore...");
    // Copper ore is usually underground
    const foundOre = await exploreUntil(bot, new Vec3(0, -1, 0), 60, () => {
      const ore = bot.findBlock({
        matching: mcData.blocksByName.copper_ore.id,
        maxDistance: 32
      });
      return ore;
    });
    if (!foundOre) {
      bot.chat("Could not find copper ore within 60 seconds.");
      return;
    }
    // Mine copper ore (requires at least stone pickaxe)
    await mineBlock(bot, "copper_ore", 1);
    rawCount = bot.inventory.count(rawCopper.id);
    if (rawCount < 1) {
      bot.chat("Failed to obtain raw copper.");
      return;
    }
    bot.chat("Mined copper ore and obtained raw copper.");
  } else {
    bot.chat(`Already have ${rawCount} raw copper.`);
  }

  // Check for furnace nearby
  let furnaceBlock = bot.findBlock({
    matching: mcData.blocksByName.furnace.id,
    maxDistance: 32
  });
  if (!furnaceBlock) {
    bot.chat("No furnace nearby. Crafting and placing a furnace...");
    // Check if we have enough cobblestone (8 needed)
    const cobblestone = mcData.itemsByName.cobblestone;
    const cobbleCount = bot.inventory.count(cobblestone.id);
    if (cobbleCount < 8) {
      bot.chat(`Need ${8 - cobbleCount} more cobblestone. Mining stone...`);
      await mineBlock(bot, "stone", 8 - cobbleCount);
    }
    // Craft furnace
    await craftItem(bot, "furnace", 1);
    // Place furnace near the bot
    const furnacePos = bot.entity.position.offset(1, 0, 0);
    await placeItem(bot, "furnace", furnacePos);
    furnaceBlock = bot.findBlock({
      matching: mcData.blocksByName.furnace.id,
      maxDistance: 32
    });
    if (!furnaceBlock) {
      bot.chat("Failed to place furnace.");
      return;
    }
  } else {
    bot.chat("Furnace already placed nearby.");
  }

  // Check for fuel (coal)
  const coal = mcData.itemsByName.coal;
  const coalCount = bot.inventory.count(coal.id);
  if (coalCount < 1) {
    bot.chat("No coal for fuel. Mining coal...");
    await mineBlock(bot, "coal_ore", 1);
    // After mining coal ore, we get coal item
    const newCoalCount = bot.inventory.count(coal.id);
    if (newCoalCount < 1) {
      bot.chat("Failed to obtain coal.");
      return;
    }
  }

  // Smelt raw_copper
  bot.chat("Smelting raw copper...");
  await smeltItem(bot, "raw_copper", "coal", 1);

  // Verify
  if (bot.inventory.count(copperIngot.id) >= 1) {
    bot.chat("Successfully smelted 1 copper ingot.");
  } else {
    bot.chat("Failed to smelt copper.");
  }
}
