async function mineWoodLog(bot) {
    // Try to mine an oak log first
    const logTypes = ["oak_log", "birch_log", "spruce_log", "jungle_log", "acacia_log", "dark_oak_log", "mangrove_log"];
    let logMined = false;
    for (const logType of logTypes) {
        const block = bot.findBlock({
            matching: mcData.blocksByName[logType]?.id,
            maxDistance: 32,
        });
        if (block) {
            bot.chat(`Found ${logType}, mining...`);
            await mineBlock(bot, logType, 1);
            logMined = true;
            break;
        }
    }
    if (!logMined) {
        // If no log found, explore until we find one
        bot.chat("No log found nearby, exploring...");
        const directions = [
            new Vec3(1, 0, 0),
            new Vec3(-1, 0, 0),
            new Vec3(0, 0, 1),
            new Vec3(0, 0, -1),
            new Vec3(1, 0, 1),
            new Vec3(-1, 0, -1),
            new Vec3(1, 0, -1),
            new Vec3(-1, 0, 1),
        ];
        const randomDirection = directions[Math.floor(Math.random() * directions.length)];
        const foundLog = await exploreUntil(bot, randomDirection, 60, () => {
            for (const logType of logTypes) {
                const block = bot.findBlock({
                    matching: mcData.blocksByName[logType]?.id,
                    maxDistance: 32,
                });
                if (block) return block;
            }
            return null;
        });
        if (foundLog) {
            const logName = mcData.blocks[foundLog.type].name;
            bot.chat(`Found ${logName} while exploring, mining...`);
            await mineBlock(bot, logName, 1);
        } else {
            bot.chat("Failed to find a log after exploration.");
        }
    }
    bot.chat("Task completed: mined 1 wood log.");
}
